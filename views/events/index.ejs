<h2>Slots Booked</h2>
<form action="/events" method = "GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchOption.name %>" >
    <br>
    <label>Date</label>
    <input type="date">
    <br>
    <label>Time</label>
    <input type="time">
    <button type="submit">Search</button>
</form>
<br>
<pre>Name          Date        Time</pre>
<% events.forEach (event => { %>
    <pre><%= event.name %>  <%= event.date %>  <%= event.time %> </pre>
<% })%>